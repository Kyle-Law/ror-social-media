<section class="users-section">
  <ol class="users-list">
    <% @users.each do |user| %>
      
      <li class="user-list-item">
        <span class="profile-link">
          <%= link_to user.name,  user_path(user), class: 'profile-name' %>
          <%= user_status(user)%>

        </span>
      </li>
    <% end %>
  </ol>
</section>
